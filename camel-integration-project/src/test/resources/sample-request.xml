<syncher-request xmlns="com/ipetrushin/syncher/request/jaxb/entities">
  <accounts>
    <!--1 to 5 repetitions:-->
    <account resource-name="string" login="string" password="string"/>
  </accounts>
  <resume-profile>
    <personal-info>
      <firstname>string</firstname>
      <lastname>string</lastname>
      <birth-date>2008-09-29</birth-date>
      <citizenship>
        <country-name>string</country-name>
      </citizenship>
    </personal-info>
    <contact-info>
      <email>string</email>
      <currentLocation>
        <country>
          <country-name>string</country-name>
        </country>
        <city>
          <city-name>string</city-name>
        </city>
      </currentLocation>
      <mobile-phone>string</mobile-phone>
      <extra-phone>string</extra-phone>
      <web-site>string</web-site>
      <facebook>string</facebook>
      <vkontakte>string</vkontakte>
      <skype>string</skype>
      <icq>string</icq>
      <twitter>string</twitter>
    </contact-info>
    <education-info>
      <!--Optional:-->
      <education-experience>
        <level value="string"/>
        <institution-name>string</institution-name>
        <department-name>string</department-name>
        <profession-name>string</profession-name>
        <inception-date>2014-09-19</inception-date>
        <graduation-date>2006-08-19+04:00</graduation-date>
        <location>
          <country>
            <country-name>string</country-name>
          </country>
          <city>
            <city-name>string</city-name>
          </city>
        </location>
      </education-experience>
      <!--Optional:-->
      <training-experience>
        <training-name>string</training-name>
        <organisation-name>string</organisation-name>
        <obtained-qualification>string</obtained-qualification>
        <graduation-date>2009-05-16</graduation-date>
      </training-experience>
      <!--Optional:-->
      <certificate>
        <certificate-name>string</certificate-name>
        <obtaining-date>2003-08-09+04:00</obtaining-date>
        <link>string</link>
      </certificate>
    </education-info>
    <job-experience-info>
      <!--Optional:-->
      <job-experience>
        <company-name>string</company-name>
        <from-date>2012-09-13+04:00</from-date>
        <to-date>2009-10-14</to-date>
        <industry-area>string</industry-area>
        <position>string</position>
        <company-location>
          <country>
            <country-name>string</country-name>
          </country>
          <city>
            <city-name>string</city-name>
          </city>
        </company-location>
        <responsibilities>string</responsibilities>
        <link>string</link>
      </job-experience>
    </job-experience-info>
  </resume-profile>
</syncher-request>